<div class="sessions">
  <div class="form-signin w-100 mx-auto">
    <div class="form-signin w-100 mx-auto">
      <div class="text-center ">
        <h3>Create a new account</h3>
        <p class="text-muted">
          Already have an account?
          <%- if controller_name != 'sessions' %>
            <%= link_to "Log in", new_session_path(resource_name) %><br />
          <% end %>
        </p>
      </div>
      <%= render 'form' %>
    </div>
  </div>
</div>
