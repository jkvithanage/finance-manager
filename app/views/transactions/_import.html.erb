<%= simple_form_for transaction do |f| %>
  <%= f.association :account, label_method: :acc_name, value_method: :id, prompt: "Choose account"%>
  <%= f.label :file, "Upload CSV File" %>
  <%= f.file_field :file, class: 'form-control', required: true %>
  <%= f.button :submit, 'Import', class: 'btn btn-primary mt-3' %>
<% end %>
